package com.test.java;

public class Ex25_While {
	public static void main(String[] args) {
		
		//m1();
		m2();
	}

	private static void m2() {
		//1~100까지 누적하시오.
		
		int sum = 0;
		for(int i=1; i<=100; i++) {
			sum +=i;
		}
		System.out.println(sum);
		
		sum =0;
		int num = 1;
		while(num <=100) {
			sum += num++;
		}
		
	}

	private static void m1() {
		// 1~10까지 출력하시오.
		for(int i=1; i<=10; i++) {
			System.out.println(i);
		}
		System.out.println();
		
		int num=1;
		while(num <=10) {
			System.out.println(num);
			num++;
		}
	}
}
