package com.test.java;

public class Ex11_Operator {
	public static void main(String[] args) {
		int a = 10;
		int b =3;
		
		System.out.printf("%d + %d = %d\n",a,b,a+b);
		System.out.printf("%d - %d = %d\n",a,b,a-b);
		System.out.printf("%d * %d = %d\n",a,b,a*b);
		System.out.printf("%d / %d = %d\n",a,b,a/b);
		System.out.printf("%d % %d = %f\n",a,b,a%b);
	}
}
