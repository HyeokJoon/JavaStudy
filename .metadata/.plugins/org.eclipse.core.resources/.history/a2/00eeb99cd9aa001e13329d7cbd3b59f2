package com.test.question;

public class Q006 {
	public static void main(String[] args) {
//		요구사항
//		사용자의 한달 수입을 입력받아 세후 금액을 출력하시오.
//
//		조건..
//		세금: 수입의 3.3%
		
		System.out.println("한달 수입 금액(원): ");
		
		System.out.println("세후 금액(원): 967,000원");
		System.out.println("세금(원) : 33,000원");
		
	}
}
