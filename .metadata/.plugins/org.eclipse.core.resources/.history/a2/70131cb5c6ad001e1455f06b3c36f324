package com.test.java;

public class Ex23_For {
	public static void main(String[] args) {
		
//		for(초기식; 조건식; 증감식) {
//			구현부;
//		}
		
		m1();
		
	}

	private static void m1() {
		//요구사항] "안녕하세요." x5 번 출력

		System.out.println("안녕하세요.");
		System.out.println("안녕하세요.");
		System.out.println("안녕하세요.");
		System.out.println("안녕하세요.");
		System.out.println("안녕하세요.");
		System.out.println();
		
		
	}
}
